@inproceedings{ WOS:000380543400119,
Author = {Al-Dahidi, S.},
Editor = {Nowakowski, T and Mlynczak, M and JodejkoPietruczuk, A and WerbinskaWojciechowska, S},
Title = {The use of Self-Organizing Maps for diagnosing faults in motor bearings},
Booktitle = {SAFETY AND RELIABILITY: METHODOLOGY AND APPLICATIONS},
Year = {2015},
Pages = {895-902},
Note = {PROCEEDINGS OF THE EUROPEAN SAFETY AND RELIABILITY CONFERENCE (ESREL),
   Wroclaw, POLAND, SEP 14-18, 2014},
Organization = {Wroclaw Univ Technol; Polish Safety \& Reliabil Assoc; European Safety
   \& Reliabil Assoc},
Abstract = {In this paper, the use of Self-Organizing Maps (SOM) for fault
   diagnostics is proposed. The method is tested with an artificial case
   study representing three start-up faulty behaviors of equipment and
   then, applied for diagnosing faults in motor bearings: the data have
   been collected from Case Western Reserve University Bearing Data Center
   with three failure modes. The method has been tested and compared to the
   K-Nearest Neighbor (KNN) method. The obtained results show that the SOM
   algorithm is a powerful visualization tool able to recognize the fault
   classes with a low misclassification rate. Furthermore, the quality of
   the SOM has been assessed using two measures: average quantization error
   and topographic error and their sensitivity with respect to the map size
   is investigated.},
