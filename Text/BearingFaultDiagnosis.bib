Title = {Rolling bearing vibration signal analysis based on dual-entropy, holder
   coefficient and gray relation theory},
Booktitle = {2017 IEEE INTERNATIONAL CONFERENCE ON SOFTWARE QUALITY, RELIABILITY AND
